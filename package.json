  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "build:data-configs": "babel-node ./src/tasks/buildDataConfigs.js",
    "build:templates": "handlebars src/templates -f src/app/configs/templates.handlebars --min --extension html -c handlebars/runtime",
    "lint:html": "pa11y-ci ./src/**/*.html",
    "lint:js": "eslint src/app/**/*.js",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "prebuild": "yarn build:templates",
    "start": "webpack --config webpack.config.prod.js && http-server ./dist -o",
    "start:dev": "webpack-dev-server --config webpack.config.dev.js",
    "sync-s3": "aws s3 sync ./dist s3://xyz.vitafit.vn && aws cloudfront create-invalidation --distribution-id XXXXXXXXXXXXX --paths \"/*\"",
    "watch:templates": "watch 'yarn build:templates' src/templates"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^9.4.8",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.0.0",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pa11y-ci": "^2.1.1",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "url-loader": "^1.1.2",
    "watch": "^1.0.2",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
