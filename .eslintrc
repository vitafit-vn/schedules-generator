{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "rules": {
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "warn"
        ]
      }
    ]
  }
}